\ProvidesPackage{fastl2007-commands}

\RequirePackage{xstring}

\newcommand{\mytable}[1]{
  \begin{tabular}{c c c c}
    \toprule
    Adjusted $R^2$ & $w_g$ & $w_h$ \\
    \midrule
    \mytablecontents{#1}
    \bottomrule
  \end{tabular}
  \caption{Adjusted $R^2$ and model weights for \mbox{#1} response curve}
  \label{tab:#1}
}

\newcommand{\mytablecontents}[1]{
  \IfStrEqCase{#1}{
    {AM-fm}{\mytablecontentsamfm}
    {AM-fc}{\mytablecontentsamfc}
    {AM-SPL}{\mytablecontentsamspl}
    {AM-md}{\mytablecontentsammd}
    {FM-fm}{\mytablecontentsfmfm}
    {FM-fc}{\mytablecontentsfmfc}
    {FM-SPL}{\mytablecontentsfmspl}
    {FM-df}{\mytablecontentsfmdf}
  }
}

\newcommand{\tablepair}[2]{
  \begin{table}[ht]
    \newcommand{\myminipagewidth}{0.45\textwidth}
    \centering
    \myminipage{\mytable{#1}}{\myminipagewidth}
    \myminipage{\mytable{#2}}{\myminipagewidth}
  \end{table}
}

\newcommand{\myminipage}[2]{
    \begin{minipage}{#2}
      \centering
      #1
    \end{minipage}
}

\newcommand{\myfigure}[2]{
  \centering
  \includegraphics[width=\textwidth]{#1.eps}
  \caption{#2}
  \label{fig:#1}
}

\newcommand{\figurepair}[4]{
  \begin{figure}[h!]
    \newcommand{\myminipagewidth}{0.45\textwidth}
    \centering
    \myminipage{\myfigure{#1}{#2}}{\myminipagewidth}
    \myminipage{\myfigure{#3}{#4}}{\myminipagewidth}
  \end{figure}
}

\newcommand{\figurequad}[8]{
  \begin{figure}[h!]
    \newcommand{\myminipagewidth}{0.45\textwidth}
    \centering
    \myminipage{\myfigure{#1}{#2}}{\myminipagewidth}
    \myminipage{\myfigure{#3}{#4}}{\myminipagewidth}
    \myminipage{\myfigure{#5}{#6}}{\myminipagewidth}
    \myminipage{\myfigure{#7}{#8}}{\myminipagewidth}
  \end{figure}
}

